<main>
    <form action="/calendar" method="POST">
        <input type="text" name="eventName">
        <button type="submit">Add Event</button>
    </form>
    <% if (events.length > 0) { %>
    <div>
        <h1>Events</h1>
        <% for (let event of events) { %>
        <div>
            <h2><%= event.name %></h2>
            <p><%= event.id %></p>
        </div>
        <% } %>
    </div>
    <% } else { %>
    <h1>No Events</h1>
    <% } %>
</main>

